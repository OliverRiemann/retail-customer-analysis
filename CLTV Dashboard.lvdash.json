{
  "datasets": [
    {
      "name": "ea221bd8",
      "displayName": "Untitled dataset",
      "queryLines": [
        "USE CATALOG `portfolio`;\n",
        "USE SCHEMA `cltv_schema`;\n",
        "\n",
        "SELECT CAST(CustomerID AS STRING) AS CustomerID, CLTV\n",
        "FROM delta.`/Volumes/portfolio/cltv_schema/portfolio_crm/cltv_data`\n",
        "ORDER BY CLTV DESC\n",
        "LIMIT 10;"
      ]
    },
    {
      "name": "17afc6bd",
      "displayName": "Untitled dataset",
      "queryLines": [
        "USE CATALOG `portfolio`;\n",
        "USE SCHEMA `cltv_schema`;\n",
        "\n",
        "SELECT CLTV\n",
        "FROM delta.`/Volumes/portfolio/cltv_schema/portfolio_crm/cltv_data`\n",
        "WHERE CLTV > 1e6 AND CLTV < 5e6;\n"
      ]
    },
    {
      "name": "07aa4920",
      "displayName": "Untitled dataset",
      "queryLines": [
        "USE CATALOG `portfolio`;\n",
        "USE SCHEMA `cltv_schema`;\n",
        "\n",
        "SELECT \n",
        "  FLOOR(CLTV / 1000000) * 1000000 AS CLTV_Bin,\n",
        "  COUNT(*) AS Count\n",
        "FROM delta.`/Volumes/portfolio/cltv_schema/portfolio_crm/cltv_data`\n",
        "WHERE CLTV BETWEEN 1000000 AND 5000000\n",
        "GROUP BY CLTV_Bin\n",
        "ORDER BY CLTV_Bin;\n"
      ]
    }
  ],
